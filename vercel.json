{
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" },
    { "src": "frontend/dist/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1.js" },
    { "src": "/frontend/(.*)", "dest": "/frontend/$1" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/frontend/dist/index.html" }
  ]
}
